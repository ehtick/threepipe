{
    "name": "@threepipe/plugin-timeline-ui",
    "description": "Timeline UI Plugin for ThreePipe",
    "version": "0.1.1",
    "devDependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "dependencies": {
        "threepipe": "file:./../../src/"
    },
    "overrides": {
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./dist/": {
            "import": "./dist/",
            "require": "./dist/"
        },
        "./src/": {
            "import": "./src/",
            "require": "./src/"
        }
    },
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "source": "src/index.ts",
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "new:pack": "npm run prepare && clean-package && npm pack && clean-package restore",
        "new:publish": "git diff --exit-code --name-only HEAD * && npm run prepare && clean-package && npm publish --access public && clean-package restore && git tag plugin-timeline-ui-$npm_package_version",
        "prepare": "npm run build && npm run docs",
        "build": "rimraf dist && vite build",
        "dev": "NODE_ENV=development vite build --watch",
        "docs": "rimraf docs && npx typedoc"
    },
    "author": "repalash <palash@shaders.app>",
    "license": "Apache-2.0",
    "keywords": [
        "three",
        "three.js",
        "animation",
        "timeline",
        "threepipe",
        "plugin"
    ],
    "bugs": {
        "url": "https://github.com/repalash/threepipe/issues"
    },
    "homepage": "https://github.com/repalash/threepipe#readme",
    "repository": {
        "type": "git",
        "url": "git://github.com/repalash/threepipe.git",
        "directory": "plugins/timeline-ui"
    },
    "clean-package": {
        "remove": [
            "clean-package",
            "scripts",
            "devDependencies",
            "overrides",
            "//"
        ],
        "replace": {
            "dependencies": {},
            "peerDependencies": {
                "threepipe": "^0.0.57",
                "react": "^18.3.1",
                "react-dom": "^18.3.1"
            },
            "peerDependenciesMeta": {
                "react": {
                    "optional": true
                },
                "react-dom": {
                    "optional": true
                }
            }
        }
    }
}
